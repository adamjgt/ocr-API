# Environment
ENV=development
DEBUG=false

# Redis Configuration
REDIS_URL=redis://localhost:6379

# OCR Processing Limits
MAX_FILE_SIZE_MB=10
MAX_PDF_PAGES=20
OCR_TIMEOUT_PER_PAGE=10

# Job Queue Settings (seconds)
RESULT_TTL=86400
JOB_TIMEOUT=300

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS=10
RATE_LIMIT_WINDOW=minute

# API Key Authentication
# Set to false to disable API key requirement
API_KEY_ENABLED=true
# Comma-separated list of valid API keys
# Generate keys with: python -c "import secrets; print(secrets.token_urlsafe(32))"
API_KEYS=your-api-key-here,another-key-here
API_KEY_HEADER=X-API-Key
